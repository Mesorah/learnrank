#: .\authors\templates\authors\pages\authors.html:10
#: .\authors\templates\authors\pages\password_reset.html:9
msgid "Submit"
msgstr "Enviar"

#: .\authors\templates\authors\pages\authors.html:16
msgid "Have an account?"
msgstr "Já tem uma conta?"

#: .\authors\templates\authors\pages\authors.html:16
#: .\authors\templates\authors\pages\authors.html:18
#: .\authors\templates\authors\pages\authors.html:19
msgid "Click here"
msgstr "Clique aqui"

#: .\authors\templates\authors\pages\authors.html:18
msgid "Don't you have an account?"
msgstr "Ainda não tem uma conta?"

#: .\authors\templates\authors\pages\authors.html:19
msgid "Forgot your password?"
msgstr "Esqueceu sua senha?"

#: .\authors\templates\authors\pages\password_reset_done.html:7
msgid "Check your email"
msgstr "Verifique seu e-mail"

#: .\authors\templates\authors\pages\password_reset_done.html:8
msgid ""
"\n"
"            If an account exists with the provided email address, you will "
"receive a message with instructions to reset your password.\n"
"        "
msgstr ""
"\n"
"            Se existir uma conta associada ao endereço de e-mail fornecido, "
"você receberá uma mensagem com instruções para redefinir sua senha.\n"
"        "

#: .\authors\templates\authors\pages\password_reset_email.html:2
#, python-format
msgid ""
"You're receiving this email because you requested a password reset for your "
"user account at %(site_name)s."
msgstr ""
"Você está recebendo este e-mail porque solicitou a redefinição de senha da "
"sua conta no site %(site_name)s."

#: .\authors\templates\authors\pages\password_reset_email.html:4
msgid "Please go to the following page and choose a new password:"
msgstr "Acesse a página a seguir e escolha uma nova senha:"

#: .\authors\templates\authors\pages\password_reset_email.html:8
msgid "In case you’ve forgotten, you are:"
msgstr "Caso tenha se esquecido, você é:"

#: .\authors\templates\authors\pages\password_reset_email.html:10
msgid "Thanks for using our site!"
msgstr "Obrigado por usar o nosso site!"

#: .\authors\templates\authors\pages\password_reset_email.html:12
#, python-format
msgid "The %(site_name)s team"
msgstr "A equipe do %(site_name)s"

msgid ""
"Incorrect confirmation. Please type \"DELETE\" to delete your account."
msgstr ""
"Confirmação incorreta. Por favor, digite \"DELETE\" para deletar sua "
"conta."

msgid "Sign Up"
msgstr "Criar conta"

msgid "Login"
msgstr "Entrar"

msgid "Delete your account"
msgstr "Exclua sua conta"

msgid "Delete confirmation"
msgstr "Confirmação de exclusão da conta"

msgid "Type \"DELETE\" to permanently delete your account."
msgstr "Digite \"DELETE\" para deletar sua conta permanentemente."

msgid "Write your new password."
msgstr "Escreva sua nova senha."

msgid "Confirm your new password."
msgstr "Confirme sua nova senha."

msgid "Write your email here."
msgstr "Escreva seu e-mail aqui."

msgid "Write your username here."
msgstr "Escreva seu nome de usuário aqui."

msgid "Write your password here."
msgstr "Escreva sua senha aqui."

msgid "Username"
msgstr "Nome de usuário"

msgid "Please enter at least 4 characters."
msgstr "Por favor, insira pelo menos 4 caracteres."

msgid "Email"
msgstr "E-mail"

msgid "Password"
msgstr "Senha"

msgid "Please enter at least 8 characters."
msgstr "Por favor, insira pelo menos 8 caracteres."

msgid "Repeat password"
msgstr "Repita a senha"

msgid "Repeat your password"
msgstr "Repita sua senha"

msgid "Username is already taken."
msgstr "Este nome de usuário já está em uso."

msgid "Email is already registered."
msgstr "Este e-mail já está registrado."

msgid "Passwords do not match."
msgstr "As senhas não coincidem."

msgid "The password must contain symbols."
msgstr "A senha deve conter símbolos."

msgid "Password must contain numbers."
msgstr "A senha deve conter números."

msgid "Password must contain letters."
msgstr "A senha deve conter letras."

msgid "Account created!"
msgstr "Conta criada com sucesso!"

msgid "Form invalid."
msgstr "Formulário inválido."

msgid "Success, you have logged out!"
msgstr "Você saiu da conta com sucesso!"

msgid "Account logged!"
msgstr "Você entrou na conta com sucesso!"

msgid "You cannot access this while not logged in."
msgstr "Você não pode acessar esta página sem estar logado."

msgid "Your account has been successfully deleted!"
msgstr "Sua conta foi excluída com sucesso!"

msgid "Password changed successfully!"
msgstr "Senha alterada com sucesso!"

msgid "You cannot access this while logged in."
msgstr "Você não pode acessar esta página enquanto estiver logado."

msgid "Your actual username"
msgstr "Seu nome de usuário atual"

msgid "New username"
msgstr "Novo nome de usuário"

msgid "Write your new username."
msgstr "Escreva seu novo nome de usuário."

msgid "Change information"
msgstr "Mudar informações"

msgid "Your username has been successfully updated!"
msgstr "Seu nome de usuário foi alterado com sucesso!"

msgid ""
"You need to wait %(days)s days before you can change your username again."
msgstr ""
"Você precisa esperar %(days)s dias antes de poder alterar seu nome de "
"usuário novamente."
